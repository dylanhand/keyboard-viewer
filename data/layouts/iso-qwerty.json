        { "id": "Digit9", "output": "9", "shiftOutput": "(" },
        { "id": "Digit0", "output": "0", "shiftOutput": ")" },
        { "id": "Minus", "output": "-", "shiftOutput": "_" },
        { "id": "Equal", "output": "=", "shiftOutput": "+" },
