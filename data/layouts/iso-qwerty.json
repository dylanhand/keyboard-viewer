        {
          "id": "AltLeft",
          "output": "",
          "label": "Alt",
          "width": 1.25,
          "type": "modifier"
        },
        { "id": "Space", "output": " ", "label": "", "width": 6.25, "type": "space" },
        {
          "id": "AltRight",
          "output": "",
          "label": "Alt",
          "width": 1.25,
          "type": "modifier"
        },
