    "`": {
      "a": "à",
      "e": "è",
      "i": "ì",
      "o": "ò",
      "u": "ù",
      "A": "À",
      "E": "È",
      "I": "Ì",
      "O": "Ò",
      "U": "Ù"
    }
  },
  "rows": [
    {
      "keys": [
        { "id": "Backquote", "output": "`", "shiftOutput": "¬", "width": 1.0 },
        { "id": "Digit1", "output": "1", "shiftOutput": "!" },
        { "id": "Digit2", "output": "2", "shiftOutput": "\"" },
        { "id": "Digit3", "output": "3", "shiftOutput": "£" },
