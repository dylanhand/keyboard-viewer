{
  "id": "iso-qwerty",
  "name": "ISO QWERTY",
  "platform": "macOS",
  "deadkeys": {
    "`": {
      "a": "à",
      "e": "è",
      "i": "ì",
      "o": "ò",
      "u": "ù",
      "A": "À",
      "E": "È",
      "I": "Ì",
      "O": "Ò",
      "U": "Ù"
    }
  },
  "rows": [
    {
      "keys": [
        { "id": "Backquote", "layers": { "default": "`", "shift": "¬", "alt": "¦" }, "width": 1.0 },
        { "id": "Digit1", "layers": { "default": "1", "shift": "!", "alt": "¹" } },
        { "id": "Digit2", "layers": { "default": "2", "shift": "\"", "alt": "²" } },
        { "id": "Digit3", "layers": { "default": "3", "shift": "£", "alt": "³" } },
        { "id": "Digit4", "layers": { "default": "4", "shift": "$", "alt": "€" } },
        { "id": "Digit5", "layers": { "default": "5", "shift": "%", "alt": "½" } },
        { "id": "Digit6", "layers": { "default": "6", "shift": "^" } },
        { "id": "Digit7", "layers": { "default": "7", "shift": "&", "alt": "{" } },
        { "id": "Digit8", "layers": { "default": "8", "shift": "*", "alt": "[" } },
        { "id": "Digit9", "layers": { "default": "9", "shift": "(", "alt": "]" } },
        { "id": "Digit0", "layers": { "default": "0", "shift": ")", "alt": "}" } },
        { "id": "Minus", "layers": { "default": "-", "shift": "_" } },
        { "id": "Equal", "layers": { "default": "=", "shift": "+" } },
        {
          "id": "Backspace",
          "layers": { "default": "\b" },
          "label": "⌫",
          "width": 2.0,
          "type": "modifier"
        }
      ]
    },
    {
      "keys": [
        { "id": "Tab", "layers": { "default": "\t" }, "label": "Tab", "width": 1.5, "type": "modifier" },
        { "id": "KeyQ", "layers": { "default": "q", "shift": "Q" } },
        { "id": "KeyW", "layers": { "default": "w", "shift": "W" } },
        { "id": "KeyE", "layers": { "default": "e", "shift": "E", "alt": "é", "alt+shift": "É" } },
        { "id": "KeyR", "layers": { "default": "r", "shift": "R", "alt": "®" } },
        { "id": "KeyT", "layers": { "default": "t", "shift": "T" } },
        { "id": "KeyY", "layers": { "default": "y", "shift": "Y" } },
        { "id": "KeyU", "layers": { "default": "u", "shift": "U", "alt": "ú", "alt+shift": "Ú" } },
        { "id": "KeyI", "layers": { "default": "i", "shift": "I", "alt": "í", "alt+shift": "Í" } },
        { "id": "KeyO", "layers": { "default": "o", "shift": "O", "alt": "ó", "alt+shift": "Ó" } },
        { "id": "KeyP", "layers": { "default": "p", "shift": "P" } },
        { "id": "BracketLeft", "layers": { "default": "[", "shift": "{" } },
        { "id": "BracketRight", "layers": { "default": "]", "shift": "}" } },
        {
          "id": "Enter",
          "layers": { "default": "\n" },
          "label": "Enter",
          "width": 1.3,
          "height": 2.075,
          "type": "enter"
        }
      ]
    },
    {
      "keys": [
        {
          "id": "CapsLock",
          "layers": { "default": "" },
          "label": "Caps",
          "width": 1.75,
          "type": "modifier"
        },
        { "id": "KeyA", "layers": { "default": "a", "shift": "A", "alt": "á", "alt+shift": "Á" } },
        { "id": "KeyS", "layers": { "default": "s", "shift": "S", "alt": "ß" } },
        { "id": "KeyD", "layers": { "default": "d", "shift": "D" } },
        { "id": "KeyF", "layers": { "default": "f", "shift": "F" } },
        { "id": "KeyG", "layers": { "default": "g", "shift": "G" } },
        { "id": "KeyH", "layers": { "default": "h", "shift": "H" } },
        { "id": "KeyJ", "layers": { "default": "j", "shift": "J" } },
        { "id": "KeyK", "layers": { "default": "k", "shift": "K" } },
        { "id": "KeyL", "layers": { "default": "l", "shift": "L" } },
        { "id": "Semicolon", "layers": { "default": ";", "shift": ":" } },
        { "id": "Quote", "layers": { "default": "'", "shift": "@" } },
        { "id": "Backslash", "layers": { "default": "#", "shift": "~" }, "label": "#" }
      ]
    },
    {
      "keys": [
        {
          "id": "ShiftLeft",
          "layers": { "default": "" },
          "label": "Shift",
          "width": 1.25,
          "type": "modifier"
        },
        { "id": "IntlBackslash", "layers": { "default": "\\", "shift": "|" }, "label": "\\" },
        { "id": "KeyZ", "layers": { "default": "z", "shift": "Z" } },
        { "id": "KeyX", "layers": { "default": "x", "shift": "X" } },
        { "id": "KeyC", "layers": { "default": "c", "shift": "C", "alt": "©" } },
        { "id": "KeyV", "layers": { "default": "v", "shift": "V" } },
        { "id": "KeyB", "layers": { "default": "b", "shift": "B" } },
        { "id": "KeyN", "layers": { "default": "n", "shift": "N", "alt": "ñ", "alt+shift": "Ñ" } },
        { "id": "KeyM", "layers": { "default": "m", "shift": "M", "alt": "µ" } },
        { "id": "Comma", "layers": { "default": ",", "shift": "<", "alt": "«" } },
        { "id": "Period", "layers": { "default": ".", "shift": ">", "alt": "»" } },
        { "id": "Slash", "layers": { "default": "/", "shift": "?" } },
        {
          "id": "ShiftRight",
          "layers": { "default": "" },
          "label": "Shift",
          "width": 2.75,
          "type": "modifier"
        }
      ]
    },
    {
      "keys": [
        {
          "id": "ControlLeft",
          "layers": { "default": "" },
          "label": "Ctrl",
          "width": 1.25,
          "type": "modifier"
        },
        {
          "id": "AltLeft",
          "layers": { "default": "" },
          "label": "Alt",
          "width": 1.25,
          "type": "modifier"
        },
        {
          "id": "MetaLeft",
          "layers": { "default": "" },
          "label": "⌘",
          "width": 1.25,
          "type": "modifier"
        },
        { "id": "Space", "layers": { "default": " " }, "label": "", "width": 6.25, "type": "space" },
        {
          "id": "MetaRight",
          "layers": { "default": "" },
          "label": "⌘",
          "width": 1.25,
          "type": "modifier"
        },
        {
          "id": "AltRight",
          "layers": { "default": "" },
          "label": "Alt",
          "width": 1.25,
          "type": "modifier"
        },
        {
          "id": "ControlRight",
          "layers": { "default": "" },
          "label": "Ctrl",
          "width": 1.25,
          "type": "modifier"
        }
      ]
    }
  ]
}
